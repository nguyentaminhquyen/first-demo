<article class="content items-list-page">
                    <div class="title-search-block">
                        <div class="title-block">
                            <div class="row">
                                <div class="col-md-6">
                                    <h3 class="title"> Projects <a href="/projects/new" class="btn btn-primary btn-sm rounded-s">
						Add New
					</a>
                                        <!--
				 -->
                                    </h3>
                                    <p class="title-description"> List of projects...</p>
                                </div>
                            </div>
                        </div>
                        <div class="items-search">
                            <form class="form-inline">
                                <div class="input-group"> <input type="text" class="form-control boxed rounded-s" placeholder="Search for..."> <span class="input-group-btn">
					<button class="btn btn-secondary rounded-s" type="button">
						<i class="fa fa-search"></i>
					</button>
				</span> </div>
                            </form>
                        </div>
                    </div>
                    <div class="card items">
                        <ul class="item-list striped">
                            <li class="item item-list-header hidden-sm-down">
                                <div class="item-row">
                                                                                                    <div class="item-col item-col-header item-col-title">
                                                                                                        <div> <span>Name</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-author">
                                                                                                        <div class="no-overflow"> <span>Author</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-author">
                                                                                                        <div class="no-overflow"> <span>Status</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-date">
                                                                                                        <div> <span>Start Date</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-date">
                                                                                                        <div> <span>End Date</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header fixed item-col-actions-dropdown"> </div>
                                                                                                </div>
                                                                                            </li>
                                                                                            <% @projects.each do |project| %>
                                                                                            <li class="item">
                                                                                                <div class="item-row">
                                                                                                    <div class="item-col fixed pull-left item-col-title">
                                                                                                        <div class="item-heading">Name</div>
                                                                                                        <div>
                                                                                                            <a href="<%="/projects/#{project.id}"%>" class="">
                                                                                                                <h4 class="item-title"> <%= project.name %> </h4>
                                                                                                            </a>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-author">
                                                                                                        <div class="item-heading">Author</div>
                                                                                                        <div class="no-overflow"><%= view_user_name(project.author_id)%></div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-author">
                                                                                                        <div class="item-heading">Status</div>
                                                                                                        <div class="no-overflow"><%= project_status(project.status)%></div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-date">
                                                                                                        <div class="item-heading">Started Date</div>
                                                                                                        <div class="no-overflow"> <%= project.start_time.strftime('%v')%> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-date">
                                                                                                       <div class="item-heading">Published</div>
                                                                                                       <div class="no-overflow"> <%= project.end_time.strftime('%v')%> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col fixed item-col-actions-dropdown">
                                                                                                        <div class="item-actions-dropdown">
                                                                                                            <a class="item-actions-toggle-btn"> <span class="inactive">
                                                                									<i class="fa fa-cog"></i>
                                                                								</span> <span class="active">
                                                                								<i class="fa fa-chevron-circle-right"></i>
                                                                								</span> </a>
                                                                                                            <div class="item-actions-block">
                                                                                                                <ul class="item-actions-list">
                                                                                                                    <li>
                                                                                                                        <a class="remove" href="<%="/projects/delete/#{project.id}"%>"> <i class="fa fa-trash-o "></i> </a>
                                                                                                                    </li>
                                                                                                                    <li>
                                                                                                                        <a class="edit" href="<%="/projects/edit/#{project.id}"%>"> <i class="fa fa-pencil"></i> </a>
                                                                                                                    </li>
                                                                                                                </ul>
                                                                                                            </div>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </li>
                                                                                            <% end %>


                        </ul>
                    </div>

                    <nav class="text-xs-right">
                        <%=will_paginate @projects%>
                    </nav>
                </article>
