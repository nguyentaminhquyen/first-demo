<article class="content items-list-page">
                    <div class="title-search-block">
                        <div class="title-block">
                            <div class="row">
                                <div class="col-md-6">
                                    <h3 class="title"> Projects <a href="/projects/new" class="btn btn-primary btn-sm rounded-s">
						Add New
					</a>
                                        <!--
				 -->
                                    </h3>
                                    <p class="title-description"> List of projects...</p>
                                </div>
                            </div>
                        </div>
                        <div class="items-search">
                            <form action="/projects" class="form-inline" method="get">
                                <div class="input-group"> <input type="text" name ="proj_name" class="form-control boxed rounded-s" placeholder="Search for..." value=""> <span class="input-group-btn">
		<button class="btn btn-secondary rounded-s" type="submit">
		      <i class="fa fa-search"></i>
		</button>
		</span> </div>
                            </form>
                        </div>
                    </div>
                    <div class="card items">
                        <ul class="item-list striped">
                            <li class="item item-list-header hidden-sm-down">
                                <div class="item-row">
                                                                                                    <div class="item-col item-col-header item-col-title">
                                                                                                        <div> <span>Name</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-author">
                                                                                                        <div class="no-overflow"> <span>Author</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-author">
                                                                                                        <div class="no-overflow"> <span>Status</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-date">
                                                                                                        <div> <span>Start Date</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header item-col-date">
                                                                                                        <div> <span>End Date</span> </div>
                                                                                                    </div>
                                                                                                    <div class="item-col item-col-header fixed item-col-actions-dropdown"> </div>
                                                                                                </div>
                                                                                            </li>
                                                                                            <%= render partial: 'projects/projects'%>
                        </ul>
                    </div>
                    <nav class="text-xs-right">
                        <%=will_paginate @projects%>
                    </nav>
                </article>
